{
  "name": "object-formatter",
  "version": "0.0.2",
  "description": "format object safely",
  "main": "lib/object-formatter.js",
  "scripts": {
    "build": "babel src --out-dir lib --source-maps inline",
    "watch": "babel src --out-dir lib --watch --source-maps inline",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec --compilers js:babel/register && rm -rf ./coverage",
    "test": "npm run build && mocha -R spec --compilers js:babel/register --recursive test"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "object",
    "format",
    "parse",
    "parser",
    "formatter"
  ],
  "author": "airtoxin <airtoxin@icloud.com>",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.6.23",
    "coveralls": "^2.11.3",
    "istanbul": "^0.3.17",
    "mocha": "^2.2.5",
    "mocha-lcov-reporter": "0.0.2"
  },
  "dependencies": {
    "isobject": "^1.0.2",
    "lodash": "^3.10.0",
    "recursive-get": "0.0.0"
  }
}
